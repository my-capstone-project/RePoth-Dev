<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <KeyFrameSet>
        </KeyFrameSet>
        <OnSwipe motion:dragDirection="dragLeft" />
    </Transition>
    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="400dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="0dp"
            motion:layout_constraintVertical_bias="0.3"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/logoMenu1"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:layout_height="15dp"
            motion:layout_constraintStart_toEndOf="@+id/menu2"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="15dp"
            android:id="@+id/menu3"
            android:layout_marginStart="32dp"
            android:layout_marginBottom="16dp">
        </Constraint>
        <Constraint
            android:layout_height="15dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="15dp"
            android:id="@+id/menu2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="16dp">
        </Constraint>
        <Constraint
            android:id="@+id/menu1"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginBottom="16dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toStartOf="@+id/menu2"
            android:layout_marginEnd="32dp">
        </Constraint>
        <Constraint
            motion:layout_constraintEnd_toEndOf="@+id/logoMenu1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/logoMenu1"
            motion:layout_constraintStart_toStartOf="@+id/logoMenu1"
            android:layout_marginTop="72dp"
            android:id="@+id/titleMenu1"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/logoMenu1"
            android:layout_width="0dp"
            android:layout_height="400dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.46"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:id="@+id/logoMenu2"
            android:layout_width="408dp"
            android:layout_height="361dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintVertical_bias="0.076" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="@+id/logoMenu2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/logoMenu2"
            motion:layout_constraintStart_toStartOf="@+id/logoMenu2"
            android:layout_marginTop="32dp"
            android:id="@+id/titleMenu2"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            motion:layout_constraintHorizontal_bias="1.0" />
        <Constraint
            android:id="@+id/menu1"
            motion:layout_constraintEnd_toStartOf="@+id/menu2"
            android:layout_width="15dp"
            android:layout_height="15dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            android:layout_marginEnd="32dp"
            motion:layout_editor_absoluteX="131dp"
            motion:layout_editor_absoluteY="688dp">
        </Constraint>
        <Constraint
            android:id="@+id/menu2"
            android:layout_width="15dp"
            android:layout_height="15dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="16dp">
        </Constraint>
        <Constraint
            motion:layout_constraintStart_toEndOf="@+id/menu2"
            android:layout_width="15dp"
            android:layout_height="15dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/menu3"
            android:layout_marginStart="32dp"
            android:layout_marginBottom="16dp">
        </Constraint>
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/logoMenu1"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="@+id/logoMenu1"
            android:layout_width="0dp"
            motion:layout_constraintTop_toBottomOf="@+id/logoMenu1"
            android:id="@+id/titleMenu1" />
    </ConstraintSet>
    <ConstraintSet
        android:id="@+id/landing3"
        motion:deriveConstraintsFrom="@+id/end">

        <Constraint
            android:id="@+id/logoMenu2"
            android:layout_width="0dp"
            android:layout_height="400dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.3"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:id="@+id/logoMenu3"
            android:layout_width="411dp"
            android:layout_height="389dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.339"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="@+id/logoMenu3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/logoMenu3"
            motion:layout_constraintStart_toStartOf="@+id/logoMenu3"
            android:layout_marginTop="16dp"
            android:id="@+id/titleMenu3"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:id="@+id/menu1"
            motion:layout_constraintEnd_toStartOf="@+id/menu2"
            android:layout_width="15dp"
            android:layout_height="15dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            android:layout_marginEnd="32dp">
        </Constraint>
        <Constraint
            android:id="@+id/menu2"
            android:layout_width="15dp"
            android:layout_height="15dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="16dp">
        </Constraint>
        <Constraint
            motion:layout_constraintStart_toEndOf="@+id/menu2"
            android:layout_width="15dp"
            android:layout_height="15dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/menu3"
            android:layout_marginStart="32dp"
            android:layout_marginBottom="40dp"
            android:layout_marginLeft="32dp">
        </Constraint>
        <Constraint
            android:id="@+id/start_btn"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="32dp"
            motion:layout_constraintBottom_toTopOf="@+id/menu2"
            android:alpha="1"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/titleMenu3"
            android:layout_marginTop="11dp"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.512" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/end"
        motion:constraintSetEnd="@+id/onboarding3" >
        <OnSwipe motion:dragDirection="dragLeft" />
    </Transition>
</MotionScene>